
# String or int element search in list
import string
number_list = [x for x in range(0,1001)] # number list
string_list = ["Shyamu","Ramu","Shivaraj","Shivni","Rajeshwar","Saroja"]  # string list
search_element = "shyamu"
# search_element = 1
def element_search(element):
    if [element for y in number_list if type(element) == int if element == y ]:
        print(f"{element} is avaliable in number_list variable")
    elif [element for x in string_list if type(element) == str if element.lower() == x.lower() ]:
        print(f"{element} is avaliable in string_list variable")
    else:
        print(f"{search_element} is not avaliable in string_list and number_list, try again !!!")

element_search(search_element)


#Single element in string or int

number = 123456
string = string.ascii_letters
search_element = "Z"
# search_element = 4
def element_search(element):
    if element in str(number):
        print(f"{element} is avaliable in number variable")
    elif element in string:
        print(f"{element} is avaliable in string variable")
    else:
        print(f"{search_element} is not avaliable in string_list and number_list, try again !!!")

element_search(search_element)

